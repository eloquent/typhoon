{
    "name": "eloquent/typhoon",
    "description": "Simple, flexible type-checking for PHP.",
    "keywords": ["type","hint","hinting","check","checking"],
    "homepage": "https://github.com/eloquent/typhoon",
    "license": "MIT",
    "authors": [
        {
            "name": "Erin Millard",
            "email": "ezzatron@gmail.com",
            "homepage": "http://ezzatron.com/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "eloquent/asplode": "~1",
        "eloquent/blox": "~1",
        "eloquent/cosmos": "~2",
        "eloquent/enumeration": "~3",
        "eloquent/typhax": "dev-master",
        "icecave/isolator": "~2",
        "icecave/pasta-ast": "~0.1",
        "icecave/rasta": "~0.1",
        "symfony/console": "~2",
        "symfony/filesystem": "~2"
    },
    "require-dev": {
        "eloquent/liberator": "~1",
        "ezzatron/phpunit-extensions": "~1",
        "icecave/testing": "~2",
        "icecave/woodhouse": "*"
    },
    "autoload": {
        "psr-0": {
            "Eloquent\\Typhoon\\TypeCheck": "src-typhoon",
            "Eloquent\\Typhoon": "src"
        }
    },
    "bin": [
        "bin/typhoon"
    ]
}
