{
    "name": "eloquent/typhoon",
    "description": "Simple, flexible type-checking for PHP.",
    "keywords": ["type","hint","hinting","check","checking"],
    "homepage": "https://github.com/eloquent/typhoon",
    "license": "MIT",
    "authors": [
        {
            "name": "Erin Millard",
            "email": "ezzatron@gmail.com",
            "homepage": "http://ezzatron.com/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "eloquent/asplode": "1.*",
        "eloquent/blox": "1.*",
        "eloquent/cosmos": "1.*",
        "eloquent/typhax": ">=0.2",
        "icecave/isolator": ">=2.1,<3",
        "icecave/visita": "0.1.*",
        "symfony/console": "2.*"
    },
    "require-dev": {
        "phake/phake": "1.*",
        "eloquent/liberator": "1.*"
    },
    "suggest": {
        "ext-sockets": "*"
    },
    "autoload": {
        "psr-0": {
            "Eloquent\\Typhoon": "src",
            "Typhoon": "src-deploy",
            "Typhoon\\Eloquent\\Typhoon": "src-typhoon"
        }
    },
    "bin": [
        "bin/typhoon"
    ]
}
