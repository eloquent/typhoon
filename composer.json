{
    "name": "eloquent/typhoon",
    "description": "Simple, flexible type-checking for PHP.",
    "keywords": ["type","hint","hinting","check","checking"],
    "homepage": "https://github.com/eloquent/typhoon",
    "license": "MIT",
    "authors": [
        {
            "name": "Erin Millard",
            "email": "ezzatron@gmail.com",
            "homepage": "http://ezzatron.com/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "eloquent/asplode": "*",
        "eloquent/blox": "*",
        "eloquent/cosmos": "*",
        "eloquent/enumeration": "*",
        "eloquent/typhax": "*",
        "icecave/isolator": "*",
        "icecave/pasta-ast": "dev-master@dev",
        "icecave/rasta": "dev-master@dev",
        "icecave/visita": "*",
        "symfony/console": "*"
    },
    "require-dev": {
        "phake/phake": "*",
        "eloquent/liberator": "*",
        "ezzatron/phpunit-extensions": "*"
    },
    "suggest": {
        "ext-sockets": "*"
    },
    "autoload": {
        "psr-0": {
            "Eloquent\\Typhoon": "src",
            "Typhoon": "src-deploy",
            "Typhoon\\Eloquent\\Typhoon": "src-typhoon"
        }
    },
    "bin": [
        "bin/typhoon"
    ],
    "extra": {
        "typhoon": {
            "outputPath": "src-typhoon",
            "sourcePaths": ["src"],
            "useNativeCallable": false
        }
    }
}
