{
    "name": "eloquent/typhoon",
    "type": "library",
    "description": "Simple, flexible type-checking for PHP.",
    "keywords": ["type","hint","hinting","check","checking"],
    "homepage": "https://github.com/eloquent/typhoon",
    "license": "MIT",
    "authors": [
        {
            "name": "Erin Millard",
            "email": "ezzatron@gmail.com",
            "homepage": "http://ezzatron.com/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "eloquent/typhax": "dev-master@dev"
    },
    "require-dev": {
        "phake/phake": "1.*"
    },
    "suggest": {
        "ext-sockets": "*"
    },
    "autoload": {
        "psr-0": {
            "Eloquent": "src"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "phake/phake",
                "version": "1.0.2",
                "dist": {
                    "url": "https://github.com/mlively/Phake/zipball/v1.0.2",
                    "type": "zip"
                },
                "autoload": {
                    "classmap": ["src"]
                }
            }
        }
    ]
}
